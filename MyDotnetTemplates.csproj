<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
      <PackageType>Template</PackageType>
      <PackageVersion>1.0.4
      </PackageVersion>
      <PackageId>Galamome.Utility.Templates</PackageId>
      <Title>Guillaume ALAMOME Templates</Title>
      <Authors>Guillaume ALAMOME</Authors>
      <Description>Templates to use when creating a ASP .Net Core Web application.</Description>
      <PackageTags>dotnet-new;templates;contoso</PackageTags>
      <TargetFramework>net6.0</TargetFramework>
  
      <IncludeContentInPack>true</IncludeContentInPack>
      <IncludeBuildOutput>false</IncludeBuildOutput>
      <ContentTargetFolders>content</ContentTargetFolders>
    </PropertyGroup>
  
    <ItemGroup>
      <Content Include="templates\**\*" Exclude="templates\**\bin\**;templates\**\obj\**" />
      <Compile Remove="**\*" />
    </ItemGroup>
  
  </Project>
  